[user]
    email = warpmatrixlyk@gmail.com
    name = warpmatrix
[color]
    branch = auto
    diff = auto
    interactive = auto
    status = auto
    ui = auto
[alias]
    stat = status --short --branch
    st = stash
    rs = reset
    co = checkout
    cm = commit
    br = branch
    rb = rebase
    lg = log --graph --decorate --oneline
    unstage = reset HEAD --
[core]
    editor = vim
[filter "lfs"]
	required = true
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
